{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "openrouter",
  "title": "Openrouter",
  "description": "Interact with OpenRouter AI models directly from your command bar",
  "icon": "command-icon.png",
  "author": "Devin",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "ask",
      "title": "Ask Question",
      "subtitle": "OpenRouter",
      "description": "Ask OpenRouter AI a question",
      "mode": "view"
    },
    {
      "name": "conversation",
      "title": "Conversation",
      "subtitle": "OpenRouter",
      "description": "Have a conversation with OpenRouter AI",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "apiKey",
      "title": "API Key",
      "description": "Your OpenRouter API key",
      "type": "password",
      "required": true
    },
    {
      "name": "defaultModel",
      "title": "Default Model",
      "description": "The default model to use",
      "type": "textfield",
      "default": "openai/gpt-3.5-turbo",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.92.1",
    "openai": "^4.85.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "~20.8.10",
    "@types/react": "^18.3.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.5.2",
    "sharp": "^0.33.5",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
